{
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "use",
          "forward",
          "import",
          "include",
          "mixin",
          "extend",
          "each",
          "return",
          "function",
          "if",
          "for",
          "else"
        ]
      }
    ],
    "declaration-empty-line-before": null,
    "no-descending-specificity": null,
    "comment-empty-line-before": null,
    "no-invalid-position-at-import-rule": null
  }
}
